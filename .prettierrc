{
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"],
  "tailwindFunctions": ["classNames"],
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "arrowParens": "always",
  "printWidth": 90,
  "tabWidth": 2,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": ".eslintrc",
      "options": {
        "trailingComma": "none"
      }
    }
  ]
}
